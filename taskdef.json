{
    "containerDefinitions": [
        {
        "name": "nginx",
        "image": "264594923212.dkr.ecr.ap-northeast-2.amazonaws.com/mynginx:latest",
        "cpu": 0,
        "memoryReservation": 300,
        "portMappings": [
            {
            "containerPort": 80,
            "hostPort": 0,
            "protocol": "tcp"
            }
        ],
        "essential": true
        }
    ],
    "family": "webtask",
    "networkMode": "bridge",
    "compatibilities": [
        "EC2"
    ],
    "requiresCompatibilities": [
        "EC2"
    ]
}
